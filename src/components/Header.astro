---
import { Image } from "astro:assets"
import { getCollection } from "astro:content"

const navLinks = await getCollection("nav")
---

<header class="w-full">
  <!-- navbar -->
  <nav
    class:list={[
      "relative mx-auto max-w-5xl",
      "has-open:[&_#menu-closed]:hidden [&_#menu-opened]:hidden has-open:[&_#menu-opened]:block"
    ]}
  >
    <div
      class="relative z-20 rounded-t-2xl border-x-16 border-t-16 border-neutral-800 bg-neutral-800"
    >
      <div
        class="flex justify-between rounded-lg border-4 border-neutral-200 bg-neutral-700 px-4 md:border-8"
      >
        <!-- logo -->
        <div class="my-auto flex items-center">
          <a href="/" class="h-full py-2">
            <Image
              class="max-h-14 md:max-h-20 lg:max-h-full"
              src={import("../assets/logo-207x93.png")}
              alt="Cover2Cover logo"
            />
          </a>
        </div>

        <!-- primary nav -->
        <div class="z-20 hidden space-x-1 md:flex">
          {
            navLinks.map((link) => (
              <div class="group relative mb-4 flex h-28 w-20 items-end justify-center px-4">
                <a
                  href={link.data.href}
                  class:list={[
                    "peer align-text-bottom text-sm font-bold text-neutral-50 uppercase hover:text-orange-500 aria-[current]:text-orange-400"
                  ]}
                  aria-current={Astro.url.pathname === link.data.href ? "page" : undefined}
                >
                  {link.data.text}
                </a>
                <div class="absolute inset-0 mx-auto h-1/5 w-1 bg-neutral-200" />
                <svg
                  viewBox="0 0 512 512"
                  xmlns="http://www.w3.org/2000/svg"
                  xmlns:xlink="http://www.w3.org/1999/xlink"
                  xml:space="preserve"
                  style="fill-rule: evenodd; clip-rule: evenodd; stroke-linecap: round; stroke-linejoin: round; stroke-miterlimit: 1.5;"
                  class="absolute inset-0 mx-auto mt-2 h-20 w-20 px-1 text-neutral-50 peer-aria-[current]:[&_g#outer]:rotate-290"
                  aria-label="Volume knob over selected menu items"
                >
                  <g id="knob" transform="translate(-8.258 -8.258) scale(1.03226)">
                    <g
                      id="outer"
                      class="origin-center transition-[rotate] duration-700 ease-in-out group-hover:rotate-290"
