        <div class="flex items-center md:hidden">
          <button type="button" popovertarget="mobile-menu">
            <svg
              class="h-6 w-6 text-neutral-50 hover:text-neutral-200"
              xmlns="http://www.w3.org/2000/svg"
              fill="none"
              viewBox="0 0 24 24"
              stroke="currentColor"
              aria-label="Open and close menu button"
            >
              <path
                id="menu-closed"
                stroke-linecap="round"
                stroke-linejoin="round"
                stroke-width="2"
                d="M4 6h16M4 12h16M4 18h16"></path>
              <path
                id="menu-opened"
                stroke-linecap="round"
                stroke-linejoin="round"
                stroke-width="2"
                d="M6 18L18 6M6 6l12 12"></path>
            </svg>
          </button>
        </div>
      </div>
    </div>

    <!-- mobile menu -->
    <div
      class="absolute -top-90 right-5 z-10 ml-auto w-52 border-r-2 border-l-2 border-neutral-800 bg-neutral-800 pt-16 pb-4 text-right transition-[display,top] transition-discrete duration-300 ease-in-out open:top-24 md:hidden open:starting:-top-90"
      id="mobile-menu"
      popover
    >
      <div class="border-t-8 border-neutral-300">
        <div class="border-t-2 border-neutral-500 pt-4">
          {
            navLinks.map((link) => (
              <div
                class:list={[
                  "relative border-b-2 border-neutral-500",
                  "even:before:absolute even:before:inset-0 even:before:m-auto even:before:size-4 even:before:rounded-full even:before:bg-neutral-200 even:before:opacity-20"
                ]}
              >
                <a
                  href={link.data.href}
                  class:list={[
                    "block border-b-2 border-neutral-300 px-4 py-2 text-neutral-50 hover:text-neutral-200 aria-[current]:text-orange-400"
                  ]}
                  aria-current={Astro.url.pathname === link.data.href ? "page" : undefined}
                >
                  {link.data.text}
                </a>
              </div>
            ))
          }
        </div>
      </div>
    </div>
  </nav>
</header>
